<?xml version="1.0" encoding="utf-8"?>

<!-- Xmlノード作成簡素化のためのノードテンプレートを定義する -->
<TemplateNodes>
    <!-- アプリ設定xml -->
    <Configurations TemplateName="AppConfigurationsXml">
        <DataSourceFilePath/>
    </Configurations>
     
    <!-- データソースxml -->
   <Databases TemplateName="DataSourceXml">
        <Database ID="">
            <Definitions>
                <TableDefinitions>
                    <TableDefinition ID="">
                        <Name/>
                        <DataType/>
                    </TableDefinition>
                </TableDefinitions>
            </Definitions>
            <Tables>
                <Table ID="">
                    <Record ID=""/>
                </Table>
            </Tables>
        </Database>
    </Databases>
</TemplateNodes>
